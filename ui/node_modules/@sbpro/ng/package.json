{
    "name": "@sbpro/ng",
    "version": "1.4.2",
    "description": "Start Bootstrap Pro Angular Schematics",
    "main": "dist/templatizer/index.js",
    "types": "dist/templatizer/index.d.ts",
    "bin": {
        "templatizer": "./dist/templatizer/index.js"
    },
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.prod.json && npm run copy && npm run build:minify",
        "build:dev": "npm run clean && tsc -p tsconfig.json && npm run copy",
        "build:dev:watch": "npm run build && tsc -p tsconfig.json --watch",
        "build:minify": "node scripts/minify.js",
        "clean": "rm -rf dist/*",
        "copy": "./scripts/copy.sh",
        "start": "npm run build:dev:watch",
        "test": "npm run build && jasmine src/**/*_spec.js"
    },
    "keywords": [
        "start bootstrap",
        "sbpro",
        "angular",
        "schematics"
    ],
    "author": "InitPlatform",
    "schematics": "./dist/collection.json",
    "files": [
        "dist"
    ],
    "dependencies": {
        "@angular-devkit/core": "9.1.7",
        "@angular-devkit/schematics": "9.1.7",
        "chalk": "4.1.0",
        "change-case": "4.1.1",
        "commander": "5.1.0",
        "shelljs": "0.8.4",
        "ts-morph": "7.1.1"
    },
    "devDependencies": {
        "@types/jasmine": "3.5.10",
        "@types/node": "13.1.2",
        "@types/shelljs": "0.8.8",
        "jasmine": "3.5.0",
        "prettier": "2.0.5",
        "renamer": "2.0.1",
        "terser": "4.7.0",
        "tslint-plugin-prettier": "2.3.0",
        "tslint": "6.1.2",
        "typescript": "3.9.5"
    }
}
